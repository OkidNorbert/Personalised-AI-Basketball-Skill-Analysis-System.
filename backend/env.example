# Basketball AI Backend - Environment Variables

# App Settings
DEBUG=True
APP_NAME="Basketball AI Performance Analysis API"

# CORS Origins
CORS_ORIGINS=["http://localhost:5173","http://localhost:3000"]

# File Upload
MAX_UPLOAD_SIZE=524288000
UPLOAD_DIR=uploads
